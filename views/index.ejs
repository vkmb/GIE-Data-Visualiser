						[
								<%=candidate[index].Hardware_IoT.may.scale%>,
						    	<%=candidate[index].AI_ML_Data_Science.may.scale%>,
						    	<%=candidate[index].Software_Application_Development.may.scale%>, 
						    	<%=candidate[index].Industry_Product_Design.may.scale%>,
						    	<%=candidate[index].Software_Platforms.may.scale%>,
						    	<%=candidate[index].Programming_Application_Platform.may.scale%>,
						    	<%=candidate[index].IP.may.scale%>,
						    	<%=candidate[index].Innovation.may.scale%>
