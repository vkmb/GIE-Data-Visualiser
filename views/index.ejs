		    type: 'radar',
		    data: {
    			labels: [
    						'Defining Problem, Problem Decomposition and Planning Code',
    						'Computer Science Fundamentals',	
    						'Programming Techniques',
    						'Programming Languages Known',
    					],
			    datasets: [
			    	{
			    		label: "MARCH",
			        	data: pap_march_kra,
			        	backgroundColor: 'rgba(255, 99, 132, 0.2)',
			        	borderColor: 'rgba(255, 99, 132, 1)',
