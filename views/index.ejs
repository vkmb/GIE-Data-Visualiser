			var ct5 = document.getElementById('ipd-radar');
			var ct6 = document.getElementById('sps-radar');
			var ct7 = document.getElementById('pap-radar');
			var ct8 = document.getElementById('ips-radar');
			var ct9 = document.getElementById('ino-radar');
