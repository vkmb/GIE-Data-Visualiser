    										options: 
    										{
    											events: ['click'],
    											hover: {
											        	// Overrides the global setting
											            // mode: 'point'
											            showTooltips: false
											        },

											        scales: {
											            yAxes: [{
											            		ticks: {
											            			beginAtZero: true,
											            			stepSize: 1,
											            			max: 10,
											            		}
											            		}
											            		]
											        	}
									    	}
										});

